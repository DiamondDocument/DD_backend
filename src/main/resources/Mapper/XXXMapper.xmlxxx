<!--这是一个命名示例文件-->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtdd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.project.Dao.StudentDao">
    <select id="selectStudentByUsername" resultType="Student">
        select * from Student where username = #{username}
    </select>
    <insert id="registerNewStudent" parameterType="Student">
        insert into `student` (`username`, `password`) values (#{username}, #{password})
    </insert>
</mapper>